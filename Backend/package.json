{
  "name": "coachgpt-pro-backend",
  "version": "1.0.0",
  "description": "Backend for CoachGPT Pro - AI workout planning system",
  "main": "src/server.ts",
  "scripts": {
    "start": "ts-node src/server.ts",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "echo 'No build needed - running TypeScript directly'",
    "test": "chmod +x test-docker.sh && ./test-docker.sh",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "docker:up": "docker-compose -f docker-compose-backend.yml up -d",
    "docker:down": "docker-compose -f docker-compose-backend.yml down",
    "docker:logs": "docker-compose -f docker-compose-backend.yml logs -f backend",
    "docker:rebuild": "docker-compose -f docker-compose-backend.yml down && docker-compose -f docker-compose-backend.yml up -d --build"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.16.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.0",
    "@types/cors": "^2.8.18",
    "@types/express": "^4.17.22",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.19.0",
    "@types/pg": "^8.15.4",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.1",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "express",
    "api",
    "backend",
    "fitness",
    "ai",
    "workout"
  ],
  "author": "Dvir Uliel",
  "license": "MIT"
}